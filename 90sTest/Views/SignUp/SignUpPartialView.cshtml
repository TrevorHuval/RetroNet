@model LoginSignUpModel
<div class="col-sm-12 mb-4 mb-lg-0 feed">
    <div class="card">
        <div class="card-header">
            Log-in
        </div>
        <div class="card-body">
            <form method="post" action="/SignUpSuccess">
                <div class="form-group">
                    <label asp-for="LogInModel.Username">Username:</label>
                    <input asp-for="LogInModel.Username" class="from-control" />
                    <span asp-validation-for="LogInModel.Username" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="LogInModel.Password">First Name:</label>
                    <input asp-for="LogInModel.Password" class="from-control" />
                    <span asp-validation-for="LogInModel.Password" class="text-danger"></span>
                </div>
                <input type="submit" value="Log-in">
            </form>
        </div>
    </div>
</div>