@model string

<ul class="nav flex-column col-sm-3" id="sidenav">
    <li class="nav-item">
        <a class="btn mr-2 mb-2 btn-primary border-dark sidenavbutton nav-link active" asp-controller="Home" asp-action="Index">Feed</a>
    </li>
    <li class="nav-item">
        @{
            var ProfileParameters = new Dictionary<string, string>()
                                            {
                        { "username",  Model }
                    };
        }
        <a class="btn mr-2 mb-2 btn-primary border-dark sidenavbutton nav-link" asp-controller="Profile" asp-action="Index" asp-all-route-data="ProfileParameters">Profile</a>
    </li>
    <li class="nav-item">
        <form id="logoutForm" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
            <button id="logout" type="submit" class="btn mr-2 mb-2 btn-primary border-dark sidenavbutton nav-link">
                <span class="btn-text">Logout</span>
            </button>
        </form>
    </li>
</ul>

